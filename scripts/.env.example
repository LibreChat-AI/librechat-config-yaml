# API Keys for AI Model Providers
# Copy this file to .env and add your actual API keys
# Leave blank any providers you don't want to update

# 302.AI - https://302.ai/
AI302_API_KEY=your-302ai-key

# APIpie - https://apipie.ai/dashboard/profile/api-keys
APIPIE_API_KEY=your-apipie-key

# Cohere - https://dashboard.cohere.com/api-keys
COHERE_API_KEY=your-cohere-key

# DeepSeek - https://platform.deepseek.com/api_keys
DEEPSEEK_API_KEY=your-deepseek-key

# Fireworks.AI - https://fireworks.ai/
FIREWORKS_API_KEY=your-fireworks-key

# GitHub Models - https://github.com (uses GITHUB_TOKEN)
# GITHUB_TOKEN is automatically available in GitHub Actions
GITHUB_TOKEN=your-github-token

# GLHF.chat - https://glhf.chat
GLHF_API_KEY=your-glhf-key

# Groq - https://console.groq.com/keys
GROQ_API_KEY=your-groq-key

# HuggingFace - https://huggingface.co/settings/tokens
HUGGINGFACE_TOKEN=your-huggingface-token

# Hyperbolic - https://app.hyperbolic.xyz/
HYPERBOLIC_API_KEY=your-hyperbolic-key

# Kluster - https://platform.kluster.ai/apikeys
KLUSTER_API_KEY=your-kluster-key

# Mistral - https://mistral.ai/
MISTRAL_API_KEY=your-mistral-key

# NanoGPT - https://nano-gpt.com/api
# Note: NanoGPT fetches from public API, no key required
NANOGPT_API_KEY=

# NVIDIA - https://build.nvidia.com/explore/discover
# Note: NVIDIA models are fetched from public API, no key required
NVIDIA_API_KEY=

# OpenRouter - https://openrouter.ai/
# Note: OpenRouter models are fetched from public API, no key required
OPENROUTER_KEY=

# Perplexity - https://docs.perplexity.ai/docs/getting-started
# Note: Perplexity models are scraped from docs, no key required
PERPLEXITY_API_KEY=your-perplexity-key

# SambaNova - https://cloud.sambanova.ai/apis
# Note: SambaNova models are scraped from community docs, no key required
SAMBANOVA_API_KEY=your-sambanova-key

# Together.AI - https://www.together.ai/
TOGETHERAI_API_KEY=your-togetherai-key

# Unify - https://unify.ai/
UNIFY_API_KEY=your-unify-key

# X.AI (Grok) - https://api.x.ai/
XAI_API_KEY=your-xai-key

# Configuration Options
# AUTOMATED_MODE: Set to 'true' to run without interactive prompts
AUTOMATED_MODE=true

# Notification Webhook (optional)
# Add your Slack/Discord webhook URL for notifications
# NOTIFICATION_WEBHOOK=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# Railway - https://railway.app (for automated redeployment)
# Get these values from your Railway project dashboard
RAILWAY_TOKEN=your-railway-api-token
RAILWAY_PROJECT_ID=your-project-id
RAILWAY_ENV_ID=your-environment-id
RAILWAY_SERVICE_ID=your-service-id
